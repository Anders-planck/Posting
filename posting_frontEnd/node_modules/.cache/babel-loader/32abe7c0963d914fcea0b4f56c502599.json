{"ast":null,"code":"import \"core-js/modules/es.symbol.js\";\nimport \"core-js/modules/es.symbol.description.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport Title from '@/components/Title';\nimport moment from 'moment';\nexport default {\n  name: 'PostValidate',\n  props: {\n    post: {\n      type: Object,\n      require: true\n    }\n  },\n  components: {\n    Title: Title\n  },\n  data: function data() {\n    return {\n      confirm: false\n    };\n  },\n  computed: {\n    posts: function posts() {\n      return this.$store.getters.getPosts;\n    }\n  },\n  methods: {\n    assign: function assign() {\n      return {\n        title: this.post.title,\n        description: this.post.description,\n        status: this.post.online // image: {\n        //     lastModified: this.post.images.lastModified,\n        //     lastModifiedDate: this.post.images.lastModifiedDate,\n        //     name: this.post.images.name,\n        //     size: this.post.images.size,\n        //     type: this.post.images.type,\n        //     webkitRelativePath: this.post.images.webkitRelativePath\n        // }\n\n      };\n    },\n    addPost: function addPost() {\n      if (!this.confirm) return;\n      this.confirm = false;\n      this.$store.commit('addPost', this.assign());\n      this.$store.commit('resetPost');\n      this.$store.commit('setStep', 1);\n      this.$router.push('/');\n    }\n  }\n};","map":{"version":3,"sources":["PostValidate.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;AAoBA,OAAA,KAAA,MAAA,oBAAA;AACA,OAAA,MAAA,MAAA,QAAA;AACA,eAAA;AACA,EAAA,IAAA,EAAA,cADA;AAEA,EAAA,KAAA,EAAA;AACA,IAAA,IAAA,EAAA;AACA,MAAA,IAAA,EAAA,MADA;AAEA,MAAA,OAAA,EAAA;AAFA;AADA,GAFA;AAQA,EAAA,UAAA,EAAA;AACA,IAAA,KAAA,EAAA;AADA,GARA;AAWA,EAAA,IAXA,kBAWA;AACA,WAAA;AACA,MAAA,OAAA,EAAA;AADA,KAAA;AAGA,GAfA;AAgBA,EAAA,QAAA,EAAA;AACA,IAAA,KADA,mBACA;AACA,aAAA,KAAA,MAAA,CAAA,OAAA,CAAA,QAAA;AACA;AAHA,GAhBA;AAqBA,EAAA,OAAA,EAAA;AACA,IAAA,MADA,oBACA;AAEA,aAAA;AACA,QAAA,KAAA,EAAA,KAAA,IAAA,CAAA,KADA;AAEA,QAAA,WAAA,EAAA,KAAA,IAAA,CAAA,WAFA;AAGA,QAAA,MAAA,EAAA,KAAA,IAAA,CAAA,MAHA,CAIA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAXA,OAAA;AAaA,KAhBA;AAiBA,IAAA,OAjBA,qBAiBA;AACA,UAAA,CAAA,KAAA,OAAA,EAAA;AAEA,WAAA,OAAA,GAAA,KAAA;AAEA,WAAA,MAAA,CAAA,MAAA,CAAA,SAAA,EAAA,KAAA,MAAA,EAAA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,WAAA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,SAAA,EAAA,CAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA;AA1BA;AArBA,CAAA","sourcesContent":["<template>\n  <div class=\"PostValidate\">\n      <h1 class=\"title\" v-if=\"post.online\">Do you want to create this post and post it ?</h1>\n\n      <h1 class=\"title\" v-if=\"!post.online\">Do you want to create this post and post it later?</h1>\n    \n      <p class=\"confirm mt-6\">\n          <p class=\"mb-3 form-group checkbox\">\n            <label for=\"confirm\" class=\"label check\">\n                Confirm\n            </label>\n            <input type=\"checkbox\" id=\"confirm\" name=\"confirm\"  v-model=\"confirm\" class=\"confirm\" />\n        </p>\n      <div class=\"button\">\n        <button @click=\"addPost\" :class=\"{'disable': !confirm}\" :disable =\"!confirm\">Create Post</button>\n      </div>\n  </div>\n</template>\n\n<script>\nimport Title from '@/components/Title'\nimport moment from 'moment'\nexport default {\n    name:'PostValidate',\n    props:{\n        post:{\n            type:Object,\n            require:true\n        }\n    },\n    components:{\n        Title,\n    },\n    data(){\n        return {\n            confirm:false\n        }\n    },\n    computed:{\n        posts(){\n            return this.$store.getters.getPosts\n        },\n    },\n    methods:{\n        assign(){\n\n            return{\n                title: this.post.title,\n                description: this.post.description,\n                status: this.post.online,\n                // image: {\n                //     lastModified: this.post.images.lastModified,\n                //     lastModifiedDate: this.post.images.lastModifiedDate,\n                //     name: this.post.images.name,\n                //     size: this.post.images.size,\n                //     type: this.post.images.type,\n                //     webkitRelativePath: this.post.images.webkitRelativePath\n                // }\n            }\n        },\n        addPost(){\n            if(!this.confirm) return\n            \n            this.confirm=false\n            \n            this.$store.commit('addPost',this.assign())\n            this.$store.commit('resetPost')\n            this.$store.commit('setStep',1)\n            this.$router.push('/')\n        }\n    }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.confirm{\n    font-family: 'Varela Round', sans-serif;\n    font-size: 1.2rem;\n}\n.button{\n    width: 100%;\n    text-align: right;\n    margin: 1rem auto ;\n    button{\n        color: #fff;\n        font-family: 'Varela Round', sans-serif;\n        padding: 1rem 2rem;\n        background: #42b983;\n        border-radius: 5px;\n        font-size: 1.2rem;\n        &:disabled{\n            color: #e01235;\n        }\n    }\n}\n\n.disable{\n    cursor: not-allowed;\n    color: #2c3e50;\n    background: #d2d9db !important;\n}\n*{\n    margin: 0;\n}\n.title{\n        font-family: 'Hind Siliguri', sans-serif;\n        font-weight: bold;\n        font-size: 2rem;\n}\n\n\n.label{\n    font-family: 'Varela Round', sans-serif;\n    font-weight: 900;\n    margin-bottom: 2px;\n    font-size: 1.2rem;\n    position: relative;\n    text-transform: capitalize;\n    display: inline-block;\n}\n\n\n\n.check{\n    margin-right: 20px;\n}\n</style>"],"sourceRoot":"src/components/Posts/PostCreate"},"metadata":{},"sourceType":"module"}