mate__fadeInDown\"\n    leave-active-class=\"animate__animated animate__fadeInUp\">\n        <div class=\"bloc\"  key=\"1\">\n            <img v-for=\"(image,index) in images\" :key=\"index\" :src=\"image.image_url\" :alt=\"image.name\"\n                :class=\"className\" />\n            <div class=\"flou\"></div>\n        </div>\n        <div class=\"close\" @click=\"LessImage\" key=\"2\">\n            <CloseIcon size=\"40px\"/>\n        </div>\n    </transition-group>\n\n    \n\n\n\n    <!-- <transition v-if=\"showModal\" appear name=\"bounce\" enter-to-class=\"animate__animated animate__backInUp\"\n        enter-active-class=\"animate__animated animate__backInUp\"\n        leave-active-class=\"animate__animated animate__bounceOutUp\">\n        <div class=\"\">\n            <div class=\"close\">\n                <CloseIcon class=\"closeIcon\" color=\"#fff\" v-on:ToogleNavMobile=\"ToogleNavMobile\" />\n            </div> \n            hello common\n        </div>\n    </transition> -->\n</div>\n</template>\n\n<script>\nimport AddIcon from \"@/components/Icons/AddIcon\"\nimport CloseIcon from \"@/components/Icons/CloseIcon\"\nexport default {\n    name:'ImageView',\n    components:{\n        AddIcon,\n        CloseIcon\n    },\n    props:{\n        images:{\n            type:Array,\n            require:false\n        },\n        image:{\n            type:Object,\n            require:false\n        },\n        state:{\n            type:String,\n            require:false,\n            default:'list'\n        }\n    },\n    data(){\n        return{\n            with:null,\n            height:null,\n            className:null,\n            ImagePreview:[],\n            showBox:false,\n        }\n    },\n    mounted(){\n        this.getSize();\n    },\n    created(){\n        this.getArray()\n    },\n    computed:{\n        getLength(){\n            if(this.$props.images.length>6){\n                return true \n            }\n            return false\n        }\n    },\n    methods:{\n        getSize(){\n            if(this.ImagePreview){\n                if(this.ImagePreview.length==1){\n                    this.className= \"image1\"\n                    return this.className\n                }\n                if(this.ImagePreview.length==2){\n                    this.className= \"image2\"\n                    return this.className\n                }\n\n                if(this.ImagePreview.length==3){\n                    this.className= \"image3\"\n                    return this.className\n                }\n\n                if(this.ImagePreview.length==4){\n                    this.className= \"image4\"\n                    return this.className\n                }\n\n                if(this.ImagePreview.length==5){\n                    this.className= \"image5\"\n                    return this.className\n                }\n\n                if(this.ImagePreview.length>=6){\n                    this.className= \"image6\"\n                    return this.className\n                }\n            }else{\n                this.className= \"image1\"\n                return this.className\n            }\n\n        },\n        getArray(){\n            if(this.$props.images){\n                if( this.$props.images.length>6){\n                    this.$props.images.forEach((image ,index) => {\n                        if(index <6)\n                            this.ImagePreview.push(image)\n                    });\n                    console.log(this.ImagePreview)\n                }else{\n                    this.ImagePreview=this.$props.images\n                } \n            }else{\n                this.ImagePreview=[]\n            }\n        },\n        MoreImage(){\n            this.showBox=true\n            this.className='any'\n        },\n        LessImage(){\n            this.showBox=false\n            this.className=this.getSize()\n        }\n    }\n}\n</script>\n\n<style scoped lang=\"scss\">\n*{\n    margin: 0;\n    padding: 0;\n}\n\nimg {\n    object-fit: cover;\n    transform: scale(1);\n    transition: all 0.2s ease-in-out;\n    height: 400px;\n    width: 100%;\n    &:hover {\n        transform: scale(1) translateY(-13px);\n        \n    }\n}\n\n\n.add,.close{\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    width: 100%;\n    height: 100px;\n    background: rgba(0, 0, 0, 0.048);\n    margin: 10px auto ;\n    border-radius: 5px;\n    transition: all 0.3s ease-in-out;\n    cursor: pointer;\n    &:hover {\n        transform: scale(1.05);\n    }\n    span{\n        display: block;\n    }\n}\n\n\n.bloc{\n    display: flex;\n    justify-content: space-evenly !important;\n    align-items: flex-start !important;\n    flex-wrap: wrap;\n    position: relative;\n    height: 400px;\n    min-height: 400px;\n    overflow-y: auto;\n    img{\n        z-index: 10;\n        border-radius: 5px;\n        border-bottom-right-radius: 3px;\n        border-bottom-left-radius: 3px;\n        &:last-child{\n            grid-column-start: 1;\n            grid-column-end: 5;\n        }\n        &:first-child{\n            grid-column-start: 1;\n            grid-column-end: 4;\n        }\n    }\n\n    .any{\n\n        width: 49% !important;\n        height: 200px !important;\n        transition: all 0.2s ease-in-out !important;\n        margin-bottom: 0.5rem;\n        &:hover{\n            transform:scale(1);\n            z-index: 15;\n            border-radius: 10px !important;\n        }\n\n    }\n\n    .image1{\n        width: 100% !important;\n        height: 100% !important;\n    }\n\n    .image2{\n        width: 49% !important;\n        height: 100% !important;\n    }\n    .image3{\n        width: 32.33% !important;\n        height: 100% !important;\n    }\n\n    .image4{\n        width: 24% !important;\n        height: 100% !important;\n    }\n\n    .image5{\n        width: 19% !important;\n        height: 100% !important;\n    }\n\n    .image6{\n        width: 15.66% !important;\n        height: 100% !important;\n    }\n\n    .flou{\n        position:absolute;\n        background: #ffde5917;\n        top: 0;\n        left: 0;\n        right: 0;\n        bottom: 0;\n        z-index: 2;\n    }\n}\n\n</style>"],"sourceRoot":"src/components/Posts"},"metadata":{},"sourceType":"module"}