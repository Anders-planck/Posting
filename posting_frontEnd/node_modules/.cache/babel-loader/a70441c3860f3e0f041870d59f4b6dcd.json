sts/CommentsNotif'\n\n\nexport default {\n    name:'PostFooter',\n    props:{\n        reply:{\n            type:Array,\n            require:true\n        },\n        at:{\n            type:String,\n            require:true\n        },\n        like:{\n            type:Number,\n            require:true\n        }\n    },\n    created(){\n        this.getLike()\n    },\n    components:{\n        LikeIcon,\n        ShareIcon,\n        CommentsNotif\n    },\n    data(){\n        return{\n            liked:{\n                state:false,\n                nber:0,\n            },\n            showComments:false,\n        }\n    },\n    computed:{\n        getSize(){\n            return this.$props.reply.length\n        },\n        getColor(){\n            if(this.liked.state){\n                return '#e74c3c'\n            }\n        },\n        getFill(){\n            if(this.liked.state){\n                return  '#e74c3c'\n            }\n        },\n        \n    },\n    methods:{\n        toogleStateLike(){\n            this.liked.state=!this.liked.state\n            if(this.liked.state)\n                this.liked.nber++\n            else\n                this.liked.nber--\n        },\n        getLike(){\n            if(this.$props.like){\n                this.liked.nber=this.$props.like\n                return\n            }\n            this.liked.nber=0\n        },\n        \n        toogleStateComments(){\n            this.$emit('toogleStateComments')\n        }\n\n    }\n}\n</script>\n\n<style lang=\"scss\" scoped >\n*{\n    margin :0;\n    padding: 0;\n}\n.FooterActions{\n    border-radius: 2px;\n    border-left: none;\n    border-right: none;\n    border-top: none;\n    width: 100%;\n    height: 100%;\n\n    ul{\n        list-style: none;\n        display: flex;\n        justify-content: space-between;\n        align-items: center;\n        height: 100%;\n        .infor{\n            border-left: 1px solid #1e272e1a;\n            padding-left: 10px;\n         